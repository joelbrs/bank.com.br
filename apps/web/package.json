{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "pnpm relay && vite",
    "build": "pnpm relay && tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "relay": "pnpm update:schema && relay-compiler",
    "update:schema": "cd ../server && pnpm update:schema && cp ./schema/schema.graphql ../web/data/schema.graphql"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@react-input/mask": "^1.2.5",
    "@repo/ui": "workspace:*",
    "lucide-react": "^0.400.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-relay": "^17.0.0",
    "react-router-dom": "^6.24.1",
    "sonner": "^1.5.0",
    "vite-plugin-relay": "^2.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-relay": "^16.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/relay-runtime": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "graphql": "^16.9.0",
    "postcss": "^8.4.39",
    "relay-compiler": "^17.0.0",
    "relay-runtime": "^17.0.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
